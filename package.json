{
    "version": "0.1.0",
    "main": "src/server.ts",
    "license": "SEE LICENSE IN LICENSE",
    "private": true,
    "scripts": {
        "build:fe": "react-scripts build",
        "build": "rimraf ./dist && tsc -p tsconfig.json",
        "debug": "node --inspect -r ts-node/register src/server",
        "democontent": "teardown-database && sudo rimraf ./database && setup-database -- --persistant && democontent",
        "generate-compose-file": "ts-node scripts/generate-compose-file",
        "lint": "tslint -p tsconfig.json",
        "migrations": "migrate-database",
        "migrations:dev": "node -r ts-node/register -r reflect-metadata node_modules/.bin/migrate-database",
        "notify": "notify",
        "schematics": "node -r ts-node/register ./node_modules/.bin/schematics --debug=false",
        "start:dev": "rimraf ./dist && nodemon",
        "start:prod": "node dist/server",
        "start": "node -r ts-node/register -r reflect-metadata src/server",
        "test": "teardown-database && setup-database && npm run -s build && cp ormconfig.test.json dist/ormconfig.json && cross-env NODE_ENV=test mocha --colors --bail --timeout 60000 --recursive dist/tests && teardown-database",
        "test:ci": "teardown-database && setup-database && npm run -s build && cp ormconfig.test.json dist/ormconfig.json && cross-env NODE_ENV=test mocha --colors --bail --timeout 60000 --recursive dist/tests && teardown-database",
        "tsfmt": "tsfmt",
        "typeorm": "node -r ts-node/register node_modules/typeorm/cli.js",
        "import-card-set": "node -r ts-node/register src/import-card-set.ts",
        "import-card-set:prod": "node -r reflect-metadata dist/import-card-set.ts"
    },
    "lint-staged": {
        "src/**/*.ts": [
            "tslint --fix -p tsconfig.json",
            "tsfmt -r",
            "git add"
        ],
        "tests/**/*.ts": [
            "tsfmt -r",
            "git add"
        ]
    },
    "husky": {
        "hooks": {
            "pre-push": "npm run -s lint && npm run -s build && rm -R dist",
            "pre-commit": "npm run -s lint",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "name": "card-library",
    "description": "A library software for your trading card collection",
    "directories": {
        "test": "tests"
    },
    "author": "",
    "dependencies": {
        "@flyacts/backend": "0.20.1",
        "@flyacts/backend-core-entities": "0.20.1",
        "@flyacts/backend-media-management": "0.20.1",
        "@flyacts/backend-user-management": "0.20.1",
        "@flyacts/request-context": "0.20.1",
        "@flyacts/routing-controllers": "0.10.5",
        "body-parser": "1.19.0",
        "class-transformer": "0.2.3",
        "class-validator": "0.9.1",
        "config": "3.2.5",
        "cookie-parser": "1.4.4",
        "express": "4.17.1",
        "faker": "4.1.0",
        "fs-extra": "8.1.0",
        "i18next": "19.1.0",
        "isemail": "3.2.0",
        "moment": "2.24.0",
        "multer": "1.4.2",
        "pg": "7.18.1",
        "postgres": "1.0.2",
        "react": "16.12.0",
        "react-dom": "16.12.0",
        "react-i18next": "11.3.1",
        "react-table": "7.0.0-rc.15",
        "reflect-metadata": "0.1.13",
        "serialize-error": "5.0.0",
        "sqlite": "3.0.3",
        "typedi": "0.8.0",
        "typeorm": "0.2.22",
        "uid-generator": "2.0.0",
        "winston": "3.2.1",
        "zxcvbn": "4.4.2"
    },
    "devDependencies": {
        "@commitlint/cli": "8.3.5",
        "@commitlint/config-conventional": "8.3.4",
        "@datenknoten/tslint-config": "0.5.0",
        "@types/body-parser": "1.17.1",
        "@types/chai": "4.2.8",
        "@types/cls-hooked": "4.3.0",
        "@types/config": "0.0.36",
        "@types/cookie-parser": "1.4.2",
        "@types/dockerode": "2.5.21",
        "@types/express": "4.17.2",
        "@types/faker": "4.1.9",
        "@types/fs-extra": "8.0.1",
        "@types/js-yaml": "3.12.2",
        "@types/minimist": "1.2.0",
        "@types/mocha": "7.0.1",
        "@types/multer": "1.4.0",
        "@types/node-emoji": "1.8.1",
        "@types/pg": "7.14.1",
        "@types/react": "16.9.19",
        "@types/react-dom": "16.9.5",
        "@types/react-table": "7.0.5",
        "@types/sparkpost": "2.1.4",
        "@types/supertest": "2.0.8",
        "@types/zxcvbn": "4.4.0",
        "chai": "4.2.0",
        "cross-env": "7.0.0",
        "cross-fetch": "3.0.4",
        "dockerode": "3.0.2",
        "husky": "4.2.1",
        "js-yaml": "3.13.1",
        "lint-staged": "10.0.6",
        "minimist": "1.2.0",
        "mocha": "7.0.1",
        "nock": "11.7.2",
        "node-notifier-cli": "1.2.0",
        "nodemon": "2.0.2",
        "supertest": "4.0.2",
        "react-scripts": "3.3.1",
        "ts-node": "8.6.2",
        "tslint": "6.0.0",
        "typescript": "^3.8.1-rc",
        "typescript-formatter": "7.2.2",
        "typescript-tslint-plugin": "0.5.5"
    },
    "schematics": "./collection.json"
}
