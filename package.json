{
    "version": "0.2.0",
    "main": "src/server.ts",
    "license": "SEE LICENSE IN LICENSE",
    "private": true,
    "scripts": {
        "css-build": "node-sass --omit-source-map-url src/sass/styles.scss src/assets/styles.css",
        "css-watch": "npm run css-build -- --watch",
        "build:fe": "react-scripts build",
        "build": "rm -Rf ./dist && tsc -p tsconfig.json",
        "debug": "node --inspect -r ts-node/register src/server",
        "democontent": "node -r ts-node/register -r reflect-metadata node_modules/.bin/democontent",
        "generate-compose-file": "ts-node scripts/generate-compose-file",
        "lint": "tslint -p tsconfig.json",
        "migrations": "migrate-database",
        "schematics": "node -r ts-node/register ./node_modules/.bin/schematics --debug=false",
        "start:dev": "rm -Rf ./dist && nodemon --exec 'npm run start'",
        "start:prod": "node dist/server",
        "start": "node -r ts-node/register -r reflect-metadata --inspect src/server",
        "tsfmt": "tsfmt",
        "typeorm": "node -r ts-node/register node_modules/typeorm/cli.js",
        "import-card-set": "node -r ts-node/register src/import-card-set.ts",
        "import-card-set:prod": "node -r reflect-metadata dist/import-card-set"
    },
    "lint-staged": {
        "src/**/*.ts": [
            "tslint --fix -p tsconfig.json",
            "tsfmt -r",
            "git add"
        ],
        "tests/**/*.ts": [
            "tsfmt -r",
            "git add"
        ]
    },
    "husky": {
        "hooks": {
            "pre-push": "npm run -s lint && npm run -s build && rm -R dist",
            "pre-commit": "npm run -s lint",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "name": "card-library",
    "description": "A library software for your trading card collection",
    "directories": {
        "test": "tests"
    },
    "author": "",
    "dependencies": {
        "@flyacts/backend": "1.3.8",
        "@flyacts/backend-core-entities": "1.3.8",
